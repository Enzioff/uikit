{
  "name": "dnt-ap",
  "version": "1.0.0",
  "description": "A lightweight CSS/JS framework for a basic ui",
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./styles": "./dist/styles.css",
    "./styles/scss": "./src/scss/index.scss",
    "./package.json": "./package.json"
  },
  "sideEffects": [
    "*.css",
    "*.scss",
    "dist/styles.css"
  ],
  "scripts": {
    "dev": "webpack serve --env port=3000 --hot",
    "build": "npm run build:lib && npm run build:demo",
    "build:lib": "npm run build:lib:js && npm run build:lib:styles && npm run build:types",
    "build:lib:js": "webpack --config webpack.lib.config.ts --env mode=production",
    "build:lib:styles": "webpack --config webpack.lib.styles.config.ts --env mode=production",
    "build:demo": "webpack --env mode=production",
    "build:types": "tsc --emitDeclarationOnly --outDir dist",
    "prepublishOnly": "npm run build:lib",
    "version:patch": "npm version patch -m 'Upgrade to %s'",
    "version:minor": "npm version minor -m 'Upgrade to %s'",
    "version:major": "npm version major -m 'Upgrade to %s'",
    "release": "npm run build:lib && npm run build:types && npm publish"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Enzioff/uikit.git"
  },
  "keywords": [
    "ui",
    "ux",
    "css",
    "scss",
    "framework",
    "components",
    "typescript",
    "ui-library"
  ],
  "author": "enzioff",
  "license": "MIT",
  "files": [
    "dist/",
    "src/scss/",
    "README.md"
  ],
  "dependencies": {
    "axios": "^1.13.2"
  },
  "devDependencies": {
    "@types/node": "^25.0.9",
    "@types/webpack": "^5.28.5",
    "@types/webpack-dev-server": "^4.7.1",
    "@types/webpack-watch-files-plugin": "^1.1.3",
    "autoprefixer": "^10.4.23",
    "copy-webpack-plugin": "^13.0.1",
    "css-loader": "^7.1.2",
    "html-webpack-plugin": "^5.6.5",
    "mini-css-extract-plugin": "^2.9.4",
    "postcss": "^8.5.6",
    "postcss-loader": "^8.2.0",
    "sass": "^1.97.2",
    "sass-loader": "^16.0.6",
    "style-loader": "^4.0.0",
    "ts-loader": "^9.5.4",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.3",
    "webpack": "^5.104.1",
    "webpack-cli": "^6.0.1",
    "webpack-dev-server": "^5.2.3",
    "webpack-watch-files-plugin": "^1.2.1"
  },
  "peerDependencies": {},
  "peerDependenciesMeta": {}
}
